{
    "manifest_version": 3,
    "name": "DCRouter",
    "description": "Mobile-PC routing",
    "version": "1.0",
    "background": {
        "service-worker": "worker.js"
    },
    "icons": {
        "128": "ext.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage"
    ]
}